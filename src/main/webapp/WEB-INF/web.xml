<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:javaee="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" id="WebApp_ID" version="2.4">
  <display-name>StudentsH</display-name>
  <servlet>
    <display-name>StudentAddController</display-name>
    <servlet-name>StudentAddController</servlet-name>
    <servlet-class>com.students.controller.StudentAddController
		</servlet-class>
  </servlet>
  <servlet>
    <display-name>StudentViewController</display-name>
    <servlet-name>StudentViewController</servlet-name>
    <servlet-class>com.students.controller.StudentViewController
		</servlet-class>
  </servlet>
  <servlet>
    <display-name>editStudentController</display-name>
    <servlet-name>editStudentController</servlet-name>
    <servlet-class>com.students.controller.editStudentController
		</servlet-class>
  </servlet>
  <servlet>
    <display-name>deleteStudentController</display-name>
    <servlet-name>deleteStudentController</servlet-name>
    <servlet-class>com.students.controller.deleteStudentController
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deleteStudentController</servlet-name>
    <url-pattern>/deleteStudentController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StudentAddController</servlet-name>
    <url-pattern>/StudentAddController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StudentViewController</servlet-name>
    <url-pattern>/StudentViewController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>editStudentController</servlet-name>
    <url-pattern>/editStudentController</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/jsp/index.jsp</welcome-file>
  </welcome-file-list>
</web-app>